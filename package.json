    }
  },
  "files": [
    "dist/consono.browser.cjs",
    "dist/consono.browser.cjs.map",
    "dist/consono.browser.mjs",
    "dist/consono.browser.mjs.map",
    "dist/consono.d.ts",
    "dist/consono.js",
    "dist/consono.js.map",
    "dist/consono.node.cjs",
    "dist/consono.node.cjs.map",
    "dist/consono.node.mjs",
    "dist/consono.node.mjs.map"

    "build:umd": "rollup --config rollup.config.umd.mjs",
    "docs:serve": "docsify serve docs",
    "lint": "eslint ./src/**/*.js",
    "lint:md": "remark .",
    "package:upgrade": "npx npm-check --update",
    "test": "jest",
    "test:coverage": "jest --collect-coverage"
